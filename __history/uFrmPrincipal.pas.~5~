{$R *.fmx}

procedure TFrmPrincipal.Button1Click(Sender: TObject);
begin
 edtPayLoad.Clear;
 edtPayLoad.Text:=PayLoad(edtChave.Text,edtBeneficiario.Text,edtCidade.Text,edtCodTrans.Text,StrToFloat(edtValor.Text));

end;

